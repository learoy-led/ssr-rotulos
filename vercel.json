{
    "version": 2,
    "public": true,
    "name": "ssr-rotulos",
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/index.js" }
    ],
    "functions": {
      "api/index.js": {
        "includeFiles": "dist/ssr-rotulos/browser/**"
      }
    },
     "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "dist/ssr-rotulos/browser/**", "use": "@vercel/static" }
  ]
  }